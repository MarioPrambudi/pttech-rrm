<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div id='id'><div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div></div>
    <div id='version'><div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div></div>
    <div id='transId'><div class='{style.label}'>Trans Id</div><div class='{style.value}' ui:field='transId'></div></div>
    <div id='status'><div class='{style.label}'>Status</div><div class='{style.value}' ui:field='status'></div></div>
    <div id='mfgNumber'><div class='{style.label}'>Mfg Number</div><div class='{style.value}' ui:field='mfgNumber'></div></div>
    <div id='reloadAmount'><div class='{style.label}'>Reload Amount</div><div class='{style.value}' ui:field='reloadAmount'></div></div>
    <div id='serviceProviderId'><div class='{style.label}'>Service Provider Id</div><div class='{style.value}' ui:field='serviceProviderId'></div></div>
    <div id='transCode'><div class='{style.label}'>Trans Code</div><div class='{style.value}' ui:field='transCode'></div></div>
    <div id='requestedTime'><div class='{style.label}'>Requested Time</div><div class='{style.value}' ui:field='requestedTime'></div></div>
    <div id='tngKey'><div class='{style.label}'>Tng Key</div><div class='{style.value}' ui:field='tngKey'></div></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
