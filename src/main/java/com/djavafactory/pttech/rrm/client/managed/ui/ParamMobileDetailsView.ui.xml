<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div id='id'><div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div></div>
    <div id='version'><div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div></div>
    <div id='terminalId'><div class='{style.label}'>Terminal Id</div><div class='{style.value}' ui:field='terminalId'></div></div>
    <div id='parameterFile'><div class='{style.label}'>Parameter File</div><div class='{style.value}' ui:field='parameterFile'></div></div>
    <div id='status'><div class='{style.label}'>Status</div><div class='{style.value}' ui:field='status'></div></div>
    <div id='createdBy'><div class='{style.label}'>Created By</div><div class='{style.value}' ui:field='createdBy'></div></div>
    <div id='createdTime'><div class='{style.label}'>Created Time</div><div class='{style.value}' ui:field='createdTime'></div></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
